  "scripts": {
    "dev": "NODE_ENV=development tsx server/index.ts",
    "dev:next": "next dev -p 3000",
    "dev:hybrid": "bash start-hybrid.sh",
    "build": "next build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "build:next": "next build",
    "start": "bash start-production.sh",
    "start:next": "next start -p 3000",
    "start:web": "next start -p 3000",
    "start:api": "NODE_ENV=production API_PORT=3001 node dist/index.js",
    "start:all": "npm-run-all --parallel start:api start:web",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:reload": "pm2 reload ecosystem.config.js",
    "pm2:delete": "pm2 delete ecosystem.config.js",
    "pm2:logs": "pm2 logs",
    "pm2:monit": "pm2 monit",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "db:seed": "tsx server/seed.ts",
    "db:export": "pg_dump $DATABASE_URL > database-export.sql && echo '✅ Database exported to database-export.sql'",
    "db:import": "psql $DATABASE_URL < database-export.sql && echo '✅ Database imported from database-export.sql'",
    "postinstall": "node scripts/auto-setup.js || true"
  },
